{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": 1,
        "curly": [2, "multi-line"],
        "comma-dangle": [2, "never"],
        "no-whitespace-before-property": 2
    }
}
